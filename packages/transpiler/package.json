{
  "name": "@jsrs/transpiler",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  },
  "dependencies": {
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@jsrs/parser": "workspace:*",
    "vscode-languageserver-textdocument": "^1.0.12"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "dev": "tsup --watch src",
    "prepublishOnly": "npm run build"
  },
  "vitest": {
    "include": [
      "test/**/*.test.ts"
    ]
  },
  "devDependencies": {
    "tsup": "^8.3.6"
  }
}
